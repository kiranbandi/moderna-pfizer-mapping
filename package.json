        "node-sass": "^4.14.1",
        "phylotree": "^1.0.0-alpha.22",
        "rc-slider": "^9.4.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-loading": "^2.0.3",
        "react-redux": "^5.0.7",
        "react-select": "^3.1.0",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0"
    },
